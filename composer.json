{
    "name": "fsi/teryt-database-bundle",
    "type": "symfony-bundle",
    "description": "Teryt database bundle",
    "keywords": ["teryt", "bundle", "symfony", "fsi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "symfony/framework-bundle": "^4.4|^5.4|^6.4",
        "symfony/console": "^4.4|^5.4|^6.4",
        "symfony/dom-crawler": "^4.4|^5.4|^6.4",
        "symfony/css-selector": "^4.4|^5.4|^6.4",
        "doctrine/persistence": "^1.3|^2.0|^3.0",
        "hobnob/xml-stream-reader": "^1.0.8",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "robrichards/wse-php": "^2.0",
        "robrichards/xmlseclibs": "^3.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "beberlei/assert": "^3.3",
        "caciobanu/behat-deprecation-extension": "^2.1",
        "doctrine/doctrine-bundle": "^1.12.8|^2.1",
        "doctrine/orm": "^2.8",
        "behat/behat": "^3.3",
        "behat/symfony2-extension": "^2.0",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.3",
        "phpspec/phpspec": "^5.0|^6.0|^7.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan-symfony": "^1.3",
        "phpstan/phpstan-beberlei-assert": "^1.1"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\TerytDatabaseBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "spec\\": "spec" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    }
}
